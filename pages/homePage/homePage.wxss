/* pages/homePage/homePage.wxss */

page {
  background-color: #f7f7f7;
}

.main {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  flex-direction: column;
  flex-wrap: wrap;
  background-color: #f7f7f7;
}

.positioning-box {
  width: 100%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.positioning {
  width: 60rpx;
  height: 60rpx;
  background-color: #fff;
  border-radius: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 9;
}

.positioning-img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
}

.choice-box {
  width: 100%;
  position: relative;
  top: -26rpx;
  display: flex;
  justify-content: center;
}

.choice {
  width: 92%;
  border-radius: 24rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.choice-gap {
  width: 100%;
  height: 24rpx;
}

.region {
  width: 100%;
  display: flex;
  justify-content: center;
}

.choice-text {
  font-size: 14px;
}

.orange {
  color: #fc943f;
}

.choice-gap-2 {
  width: 100%;
  height: 16rpx;
}

.inform {
  width: 96%;
  font-size: 24rpx;
  color: gray;
  text-align: center;
}

.referral {
  width: 78%;
  display: flex;
  justify-content: space-around;
}

.button {
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.button-box {
  width: 42%;
  height: 92rpx;
  border-radius: 12rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
}

.button-img {
  width: 40rpx;
  height: 40rpx;
}

.button-text-box {
  display: flex;
  justify-content: center;
  margin-left: 20rpx;
}

.button-text {
  font-size: 28rpx;
  text-align: center;
}

.frame {
  width: 72rpx;
  border-radius: 40rpx;
  background-color: #fc943f;
  color: #fff;
  margin-left: 20rpx;
}

.tap-box {
  width: 100%;
  height: 110rpx;
  display: flex;
  justify-content: center;
}

.tap {
  width: 92%;
  display: flex;
  justify-content: flex-end;
}

.main-tap-box {
  width: 100%;
  border-radius: 20rpx;
  background-color: #fff;
  height: 200rpx;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.main-tap-img {
  width: 30rpx;
  height: 30rpx;
  margin-top: 18rpx;
  position: absolute;
  right: 114rpx;
}

.choice-gap-4 {
  width: 100%;
  height: 20rpx;
}

.main-tap-bg {
  width: 88%;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.main-tap {
  margin:10rpx 20rpx;
  width: 100%;
  display: flex;
  justify-content: flex-start;
}

.main-tap image {
  width: 30rpx;
  height: 30rpx;
}

.main-tap view {
  font-size: 28rpx;
}

.tap-img {
  width: 40rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.tap-img image {
  width: 50rpx;
  height: 50rpx;
}

.choice-gap-3 {
  width: 100%;
  height: 30rpx;
}

.aomunt {
  width: 36rpx;
  height: 36rpx;
  margin-left: 12rpx;
  border-radius: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  background-color: #fc943f;
  color: #fff;
}

.bottom {
  width: 100%;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
}

.bottom-box {
  width: 280rpx;
  height: 280rpx;
  margin-bottom: 50rpx;
  border-radius: 100rem;
  background-color: #fc943f;
  color: #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
  z-index: 9;
}

.bottom-box image {
  width: 160rpx;
  height: 160rpx;
  margin-bottom: -128rpx;
}

.bottom-img {
  width: 100%;
  position: absolute;
  bottom: 100rpx;
}

.bottom-img image {
  width: 160rpx;
  height: 160rpx;
}

.window-box {
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
  display: flex;
  justify-content: center;
  background-color: rgba(75, 73, 73, 0.747);
  z-index: 99;
}

.window {
  width: 92%;
  position: absolute;
  bottom: 0;
}

.window-img {
  width: 700rpx;
  height: 700rpx;
}

.bind-tap {
  width: 100%;
  height: 100%;
}

.window-box-error {
  width: 100%;
  height: 100%;
  position: absolute;
  bottom: 0;
  background-color: rgba(75, 73, 73, 0.747);
  z-index: 99;
}

.window-error {
  width: 100%;
  height: 350rpx;
  margin-top: 550rpx;
  display: flex;
  justify-content: center;

  position: relative;
}

.window-box {
  width: 500rpx;
  height: 350rpx;
  position: absolute;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;

}

.window-error-top {
  width: 100%;
  height: 290rpx;
  background-color: #FFF;
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  align-content: flex-start;
}
.window-error-top view{
  width: 92%;
  height: 30rpx;
  text-align: center;
  margin-top: 20rpx;
}
.window-error-button {
  width: 100%;
  height: 60rpx;
  border-top: 1rpx solid #000;
  background-color: #FFF;
  text-align: center;
}